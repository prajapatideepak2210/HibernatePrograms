package com.bridgelabz.model;

public class Address {
	private int flateNumber;
	private String apartmentAndColony;
	private String city;
	private String state;
	private Student student;
	
	public int getFlateNumber() {
		return flateNumber;
	}
	public void setFlateNumber(int flateNumber) {
		this.flateNumber = flateNumber;
	}
	public String getApartmentAndColony() {
		return apartmentAndColony;
	}
	public void setApartmentAndColony(String apartmentAndColony) {
		this.apartmentAndColony = apartmentAndColony;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public String getState() {
		return state;
	}
	public void setState(String state) {
		this.state = state;
	}
	public Student getStudent() {
		return student;
	}
	public void setStudent(Student student) {
		this.student = student;
	}
	
}
